{
    "name": "takeru-yamamoto/laravel-presentation-domain",
    "description": "My Customized Presentation and Domain layers for Laravel",
    "type": "library",
    "license": "MIT",
    "version": "1.0",
    "autoload": {
        "psr-4": {
            "MyCustom\\PresentationDomain\\": "src",
            "MyCustom\\Controllers\\": "src/Controllers",
            "MyCustom\\Events\\": "src/Events",
            "MyCustom\\Forms\\": "src/Forms",
            "MyCustom\\Functions\\View\\": "src/Functions/View",
            "MyCustom\\Mails\\": "src/Mails",
            "MyCustom\\Services\\": "src/Services"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MyCustom\\PresentationDomain\\ServiceProvider"
            ],
            "aliases": {
                "ViewFunctions": "MyCustom\\Functions\\View\\ViewFunctions"
            }
        }
    },
    "authors": [
        {
            "name": "Takeru Yamamoto",
            "email": "yamamo9150@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.1.0",
        "laravel/framework": ">=10.0",
        "laravel/ui": "^4.2",

        "takeru-yamamoto/laravel-helpers": "1.0",
        
        "linecorp/line-bot-sdk": "^7.6",
        "twilio/sdk": "^6.44"
    },
    "minimum-stability": "dev"
}
